<body>
    <h1>List of Providers with Specialty: <%= data[0]?.provider_specialty %></h1>
    <table border="1">
      <thead>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Specialty</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach(provider => { %>
          <tr>
            <td><%= provider.first_name %></td>
            <td><%= provider.last_name %></td>
            <td><%= provider.provider_specialty %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</body>